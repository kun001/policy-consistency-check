# Client-exposed variables (Vite exposes variables prefixed with VITE_)
VITE_API_BASE_URL=
VITE_API_BASE_PREFIX=/api

# Dataset IDs (do NOT put secrets in Vite env)
VITE_TARGET_DATASET_ID=national_policy_documents
VITE_LOCAL_DATASET_ID=policy_documents

# Dev-only variables (used in vite.config.js; not exposed to client)
DEV_PROXY_TARGET=http://127.0.0.1:10010